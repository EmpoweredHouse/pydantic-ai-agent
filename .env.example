# API keys for LLM providers
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_API_KEY=your_google_api_key

# Pydantic-AI configuration
PYDANTIC_AI_DEFAULT_MODEL=openai:gpt-4o
PYDANTIC_AI_REQUEST_TIMEOUT=120

# Pydantic Logfire configuration (optional, for debugging)
LOGFIRE_TOKEN=your_logfire_token
LOGFIRE_SEND_TO_LOGFIRE=false
LOGFIRE_ENVIRONMENT=development

# Evaluation settings (optional)
PYDANTIC_EVALS_OUTPUT_DIR=./eval_results